{
  "name": "talvbansal/media-manager",
  "description": "A media browser and uploader for laravel apps written in vue js and bootstrap",
  "keywords": [
    "media",
    "media manager",
    "blog",
    "files",
    "file uploader",
    "uploader",
    "laravel",
    "easel",
    "developers",
    "minimal",
    "vue.js",
    "bootstrap"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Talvinder Bansal",
      "email": "talvbansal@hotmail.com",
      "role": "Developer"
    }
  ],
  "require": {
    "php": ">=7.0",
    "dflydev/apache-mime-types": "^1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^6.0",
    "illuminate/support": "^5.0" || "^6.0 || ^7.0",
    "illuminate/routing": "^5.0" "^6.0 || ^7.0",
    "orchestra/testbench": "~3.0",
    "mockery/mockery": "^0.9"
  },
  "autoload": {
    "psr-4": {
      "TalvBansal\\MediaManager\\": "src/"
    }
  },
  "autoload-dev": {
    "classmap": [
      "tests/TestCase.php"
    ]
  },
  "minimum-stability": "dev",
  "config": {
    "preferred-install": "source",
    "sort-packages": true
  },
  "extra": {
    "laravel": {
      "providers": [
        "TalvBansal\\MediaManager\\Providers\\MediaManagerServiceProvider"
      ]
    }
  }
}
